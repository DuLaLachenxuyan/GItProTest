<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trw.mapper.LookinfoMapper">
	<select id="findListById" resultType="com.trw.model.TrwTLookinfo">
		SELECT
		lookfee,
		num
		FROM
		trw_t_lookinfo WHERE
		productid=#{productid}
	</select>
</mapper>
