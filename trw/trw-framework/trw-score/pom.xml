	<description>积分中心</description>
	
	<properties>
		<alipay.version>3.0.1</alipay.version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.trw</groupId>
			<artifactId>trw-common-core</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.trw</groupId>
			<artifactId>trw-order-api</artifactId>
			<version>0.0.1-SNAPSHOT</version>
